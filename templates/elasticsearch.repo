[elasticsearch-{{ elasticsearch_install_version }}]
name=elasticsearch repository for {{ elasticsearch_install_version }} packages
#baseurl=http://packages.elastic.co/elasticsearch/{{ elasticsearch_install_version }}/centos
baseurl=https://artifacts.elastic.co/packages/{{ elasticsearch_install_version }}/yum
